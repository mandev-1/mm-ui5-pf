{
  "_version": "1.58.0",
  "sap.app": {
    "id": "com.mmd",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "com.mmd.view.App",
      "type": "XML",
      "async": true
    },
    "dependencies": {
      "minUI5Version": "1.100.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.f": {},
        "sap.tnt": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.mmd.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "serviceUrl": "/catalog/",
          "odataVersion": "4.0",
          "autoExpandSelect": true,
          "synchronizationMode": "None",
          "operationMode": "Server",
          "groupId": "$direct"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.mmd.view",
        "controlAggregation": "pages",
        "controlId": "pageContainer",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": "",
          "target": ["TargetHome"]
        },
        {
          "name": "RouteProjects",
          "pattern": "projects",
          "target": ["TargetProjects"]
        },
        {
          "name": "RouteTechStack",
          "pattern": "techstack",
          "target": ["TargetTechStack"]
        },
        {
          "name": "RouteArticles",
          "pattern": "articles",
          "target": ["TargetArticles"]
        }
      ],
      "targets": {
        "TargetHome": {
          "viewId": "home",
          "viewName": "Home"
        },
        "TargetProjects": {
          "viewId": "projects",
          "viewName": "Projects"
        },
        "TargetTechStack": {
          "viewId": "techstack",
          "viewName": "TechStack"
        },
        "TargetArticles": {
          "viewId": "articles",
          "viewName": "Articles"
        }
      }
    }
  }
}

